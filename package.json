{
  "name": "tile2wms",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "rm -rf ./node_modules/es5ol && yarn transpile",
    "transpile": "babel ./node_modules/ol -d ./node_modules/es5ol",
    "start": "nodemon index.js",
    "test": "jest test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmenedes/tile2wms.git"
  },
  "author": "Chris Menedes/Tim Keane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmenedes/tile2wms/issues"
  },
  "homepage": "https://github.com/cmenedes/tile2wms#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "jimp": "^0.6.4",
    "ol": "^5.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "verbose": true,
    "automock": false,
    "testMatch": [
      "**/__tests__/*.test.js",
      "**__tests__\\*.test.js"
    ],
    "transformIgnorePatterns": [
      "(?!(ol)/)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "__tests__"
    ]
  }
}
